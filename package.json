{
  "name": "doc-submitter",
  "version": "0.1.0",
  "private": true,
  "author": "Alexey Soloviev",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config=test/jest.config.js",
    "test:watch": "jest --config=test/jest.config.js --watch",
    "test:coverage": "jest --config=test/jest.config.js --coverage",
    "dev:up": "docker-compose -f docker-compose.yml -f docker-compose.development.yml up",
    "dev:down": "docker-compose -f docker-compose.yml -f docker-compose.development.yml down",
    "prod:up": "docker-compose -f docker-compose.yml -f docker-compose.production.yml up --build --detach",
    "prod:down": "docker-compose -f docker-compose.yml -f docker-compose.production.yml down"
  },
  "dependencies": {
    "@emotion/cache": "11.4.0",
    "@emotion/react": "11.4.1",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@fontsource/roboto": "4.5.0",
    "@material-ui/styles": "4.11.4",
    "@mui/icons-material": "5.0.0",
    "@mui/material": "5.0.0",
    "@mui/system": "5.0.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "8.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.1",
    "@types/react": "17.0.21",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.2.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  }
}
